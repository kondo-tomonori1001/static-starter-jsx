{
  "name": "jsx-starter",
  "version": "1.0.0",
  "description": "jsx-starter",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public",
    "server": "browser-sync start -s public -w public",
    "webpack:dev": "webpack --config webpack.dev.js --watch",
    "webpack:prod": "webpack --config webpack.prod.js",
    "lint:jsx": "eslint src/jsx/**/*.jsx",
    "lint:js": "eslint src/scripts/**/*.js",
    "sass2css": "sass src/sass/main.scss:public/styles.css",
    "sass2css:prod": "sass src/sass/main.scss:public/styles.css --no-source-map",
    "postcss": "postcss public/styles.css -o public/styles.css",
    "sass:dev": "run-s sass2css postcss",
    "sass:prod": "run-s sass2css:prod postcss",
    "imagemin:png": "imagemin src/images/*.png --out-dir=public/images --plugin.pngquant.quality=0.7 --plugin.pngquant.quality=0.8",
    "imagemin:jpeg": "imagemin src/images/*.{jpeg,jpg} --out-dir=public/images --plugin.mozjpeg.quality=0.7",
    "imagemin:gif": "imagemin src/images/*.gif --out-dir=public/images --plugin.gifsicle",
    "imagemin:svg": "imagemin src/images/*.svg --out-dir=public/images --plugin.svgo",
    "imagemin": "run-p imagemin:*",
    "dev": "run-p webpack:dev sass:dev imagemin server",
    "build": "run-p clean webpack:prod sass:prod imagemin"
  },
  "author": "kon",
  "license": "ISC",
  "volta": {
    "node": "16.13.2",
    "npm": "8.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}